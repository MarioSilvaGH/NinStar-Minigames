            <kind>1</kind>
            <string>///Definições da surface

//SE surface não existir... Re-criar
if(!surface_exists(darkness))
     darkness = surface_create(room_width,room_height);
     
//Definir surface alvo
surface_set_target(darkness);

//Cobrir tela com uma cor para subtração (Quanto mais escuro, menor a opacidade)
draw_clear_alpha($EEEEEE,1);
draw_set_blend_mode(bm_subtract);

//--- Jogador ---
var playerblink;
if(instance_exists(obj_coursesystem))
&amp;&amp;(obj_coursesystem.lleffecttrigger == 1)
    playerblink = choose(0,0,1,0,1,0,1);
else
    playerblink = 1;

if(instance_exists(obj_player))
&amp;&amp;(playerblink == 1)
    draw_sprite_ext(spr_player_light,0,obj_player.x,obj_player.y,1,1,obj_player.image_angle,c_white,1);

//Resetar predefinições
draw_set_blend_mode(bm_normal);
surface_reset_target();
</string>
