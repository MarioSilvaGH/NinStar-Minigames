            <string>///HUD

//Power-Up
draw_sprite_ext(spr_hud_bodyshadow,0,view_xview[0]+hudpos,view_yview[0]+8,1,1,0,c_black,0.1);   
draw_sprite_ext(spr_hud_body,0,view_xview[0]+hudpos,view_yview[0]+8,1,1,0,c_white,0.5);

//Super Leaf
if(global.powerup_leaf == 1)
    draw_sprite_ext(spr_hud_powerup,0,view_xview[0]+1+hudpos,view_yview[0]+9,1,1,0,c_white,0.5);
if(global.powerup_feather == 1)
    draw_sprite_ext(spr_hud_powerup,1,view_xview[0]+17+hudpos,view_yview[0]+9,1,1,0,c_white,0.5);
if(global.powerup_wing == 1)
    draw_sprite_ext(spr_hud_powerup,2,view_xview[0]+3+hudpos,view_yview[0]+19,1,1,0,c_white,0.5);

//Super Shoot
draw_sprite_ext(spr_hud_bodyshadow,1,view_xview[0]+hudpos,view_yview[0]+8,1,1,0,c_black,0.2);
draw_sprite_ext(spr_hud_body,1,view_xview[0]+hudpos,view_yview[0]+8,1,1,0,c_white,0.5);

var _ssm;
if(global.powerup_wing == 1)
    _ssm = 2;
else
    _ssm = 1;

var _ss = lerp(0,0.01,obj_player.supercharge);
if(_ss &gt; 0.01){
    draw_sprite_ext(spr_hud_supershoot,0,view_xview[0]+hudpos+50,view_yview[0]+8,1,1,0,c_orange,1);
    draw_sprite_ext(spr_hud_supershoot,1,view_xview[0]+hudpos+50,view_yview[0]+8,_ss*_ssm,1,0,c_orange,1);
}

//Vida

//Corpo
draw_sprite_ext(spr_hud_bodyshadow,2,view_xview[0]+hudpos,view_yview[0]+8,1,1,0,c_black,0.1);
draw_sprite_ext(spr_hud_body,2,view_xview[0]+hudpos,view_yview[0]+8,1,1,0,c_white,0.5-lleffect);
draw_sprite_ext(spr_hud_body,2,view_xview[0]+hudpos,view_yview[0]+8,1,1,0,c_red,lleffect);
    
//Contador
draw_sprite_ext(spr_hud_life,global.player_life,view_xview[0]+35+hudpos,view_yview[0]+25,1,1,0,c_white,0.5);

///DEBUG
if(keyboard_check_pressed(vk_escape)) game_restart();
if(keyboard_check_pressed(vk_f1)) global.powerup_leaf =! global.powerup_leaf;
if(keyboard_check_pressed(vk_f2)) global.powerup_feather =! global.powerup_feather;
if(keyboard_check_pressed(vk_f3)) global.powerup_wing =! global.powerup_wing;
if(keyboard_check_pressed(vk_f4)){

    if(instance_exists(obj_coursedarkness)){
        with(obj_coursedarkness)
            instance_destroy();    
    }
    else
        instance_create(0,0,obj_coursedarkness);
}
if(keyboard_check_pressed(vk_space)){

    if(global.player_life &gt; 0)
        global.player_life--;
    else
        global.player_life = 3;
}
var txt = "F1: Leaf #F2: Feather #F3: Wing #F4: Dark GFX ##SHIFT: Shoot #SPACEBAR: Health #ESC: Restar";

