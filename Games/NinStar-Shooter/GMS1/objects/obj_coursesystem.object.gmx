            <kind>1</kind>
            <string>///Verificações constantes do nível
speed = path_speed;

//--- Parallax ---

//Horizontal
background_x[0] = (view_xview[0]/2);

//Vertical
if(background_height[0] &lt; room_height)
&amp;&amp;(background_height[0] &gt; view_hview[0])
    background_y[0] = (view_yview[0]*(room_height-background_height[0])/(room_height-view_hview[0]));

//--- Limites da área do jogador ---

//Esquerda
if(obj_player.x &lt; view_xview[0]+16){

    //Posiciona-lo no canto da tela
    obj_player.x = view_xview[0]+16;

    //Se tentar prosseguir... Parar
    if(obj_player.hspeed &lt; 0)
        obj_player.hspeed = 0;
};
//Direita
else if(obj_player.x &gt; view_xview[0]+view_wview[0]-16){

    //Posiciona-lo no canto da tela
    obj_player.x = view_xview[0]+view_wview[0]-16;

    //Se tentar prosseguir... Parar
    if(obj_player.hspeed &gt; 0)
        obj_player.hspeed = 0;
};

//Cima
if(obj_player.y &lt; view_yview[0]+16){

    //Posiciona-lo no canto da tela
    obj_player.y = view_yview[0]+16;

    //Se tentar prosseguir... Parar
    if(obj_player.vspeed &lt; 0)
        obj_player.vspeed = 0;
};
//Baixo
else if(obj_player.y &gt; view_yview[0]+view_hview[0]-16){

    //Posiciona-lo no canto da tela
    obj_player.y = view_yview[0]+view_hview[0]-16;

    //Se tentar prosseguir... Parar
    if(obj_player.vspeed &gt; 0)
        obj_player.vspeed = 0;
};
