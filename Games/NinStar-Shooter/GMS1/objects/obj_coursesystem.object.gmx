            <string>///Controle do nível/objetos

//Animação ínicial do HUD
if(hudpos &lt; -15)
    hudpos += 15;
else
    hudpos = 0;
    
//Efeito de vida baixa
if(global.player_life &lt; 2){

    //HUD
    if(lleffecttrigger == 0){
    
        if(lleffect &lt; 0.5)
            lleffect += 0.03;
        else
            lleffecttrigger = 1;
    }
    else{
    
        if(lleffect &gt; 0)
            lleffect -= 0.03;
        else
            lleffecttrigger = 0;
    }
    
    //Fumaça (Jogaddor
    if(instance_exists(obj_player))
        instance_create(obj_player.x+random_range(-16,14),obj_player.y+random(8),obj_playe_fog);
}
else{

    lleffecttrigger = 0;
    lleffect = 0;
}
