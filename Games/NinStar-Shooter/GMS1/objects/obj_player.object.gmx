    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Controles &amp; Comportamento

//Velocidade Extra (P-WING)
if(global.powerup_wing == 1)
    extraspeed = 3;
else
    extraspeed = 0;

//CIMA e BAIXO
if(keyboard_check(global.key_up))
    vspeed -= 0.8+extraspeed;
else if(keyboard_check(global.key_down))
    vspeed += 0.8+extraspeed;
else{

    //Instábilidade na altura (Quando tiver apenas 1 vida)
    if(global.player_life == 1)
        y += 0.8;
};

//ESQUERDA e DIREITA
if(keyboard_check(global.key_left))
    hspeed -= 0.8+extraspeed;
else if(keyboard_check(global.key_right))
    hspeed += 0.8+extraspeed;
    
//Velocidade Máxima
vspeed = clamp(vspeed,-3-extraspeed,+3+extraspeed);
hspeed = clamp(hspeed,-3-extraspeed,+3+extraspeed);

//Inclinar Sky Pop
if(vspeed != 0)    
    image_angle = -vspeed*4.5;
else
    image_angle = 0;
        
//Tiros Extras (CAPE FEATHER)
var _limit;
if(global.powerup_feather == 1)
    _limit = 6;
else
    _limit = 3;
        
//Tiro (Rio de Janeiro intesifies)
if(keyboard_check_pressed(global.key_shoot)){

    //Projétil
    if(instance_number(obj_player_shoot) &lt; _limit){
        
        scr_playershoot(0);
    };
};

//Super Shoot (SUPER LEAF)
if(global.powerup_leaf == 1){
        
    //Menor tempo de carregamento (P-WING)
    var _halftime   
    if(extraspeed &gt; 0)
        _halftime = 50;
    else
        _halftime = 100;
            
    //Carregar
    if(keyboard_check(global.key_shoot)){
    
        if(supercharge &lt; _halftime)
            supercharge++;
    };
    //Soltar
    else{
    
        //Projétil
        if(instance_number(obj_player_shoot) &lt; _limit)
        &amp;&amp;(supercharge &gt;= _halftime){
            
            scr_playershoot(1);
        };
    
        //Reiniciar
        supercharge = 0;  
    };
};

//Seguir movimento da câmera
x += obj_coursesystem.hspeed;
y += obj_coursesystem.vspeed;
</string>
          </argument>
